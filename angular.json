{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ccpay-web-component": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "aot": true,
            "outputPath": {
              "base": "dist/ccpay-web-component"
            },
            "index": "src/index.html",
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./src/assets/stylesheets/main.scss"
            ],
            "scripts": [],
            "browser": "src/main.ts",
            "stylePreprocessorOptions": {
              "includePaths": [
                "."
              ]
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "ccpay-web-component:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "ccpay-web-component:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "ccpay-web-component:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "styles": [
              "styles.css",
              "./src/assets/stylesheets/main.scss"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": ".eslintrc.js",
            "lintFilePatterns": [
              "**/*.spec.ts",
              "**/*.d.ts"
            ]
          }
        }
      }
    },
    "ccpay-web-component-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "ccpay-web-component:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ccpay-web-component:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": ".eslintrc.js",
            "lintFilePatterns": [
              "**/*.spec.ts",
              "**/*.d.ts"
            ]
          }
        }
      }
    },
    "payment-lib": {
      "root": "projects/payment-lib",
      "sourceRoot": "projects/payment-lib/src",
      "projectType": "library",
      "prefix": "ccpay",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/payment-lib/tsconfig.lib.json",
            "project": "projects/payment-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/payment-lib/ng-package.prod.json",
              "tsConfig": "projects/payment-lib/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/payment-lib/src/test.ts",
            "tsConfig": "projects/payment-lib/tsconfig.spec.json",
            "karmaConfig": "projects/payment-lib/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": ".eslintrc.js",
            "lintFilePatterns": [
              "**/*.spec.ts",
              "**/*.d.ts"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "740f19be-587b-4f7e-a5ac-ba6ffff3aa41",
    "schematicCollections": ["@angular-eslint/schematics"]
  }
}
